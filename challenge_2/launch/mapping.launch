<launch>
	<!--call to the slam_mapping for the mapping  -->
	<node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
		<remap from="scan" to ="/scan"/>
		<param name="base_frame" value="base_footprint"/>
		<param name="odom_frame" value="odom"/>
	</node>
	<!-- node that corresponds to the detection algorithm -->
	<node pkg="challenge_2" name="feature_node" type="featurepoints.py" output="screen">
	</node>    		
    	
     	
</launch>
